{
	"name": "SimpleWebActivity",
	"properties": {
		"activities": [
			{
				"name": "SendToServiceBusQueue",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "POST",
					"url": "https://sb-for-test-queues.servicebus.windows.net/items-queue/messages",
					"body": {
						"message": "Hello, World2!"
					},
					"authentication": {
						"type": "MSI",
						"resource": "https://servicebus.azure.net/"
					}
				}
			}
		],
		"annotations": []
	}
}