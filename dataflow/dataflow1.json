{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ReadInputDataset",
						"type": "DatasetReference"
					},
					"name": "ReadInputFile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "CreateStageFile",
						"type": "DatasetReference"
					},
					"name": "GenertaeStageFile"
				}
			],
			"transformations": [
				{
					"name": "Validation"
				}
			],
			"scriptLines": [
				"source(output(",
				"          oktaId as string,",
				"          newFsrAccountNumber as string,",
				"          prin as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> ReadInputFile",
				"ReadInputFile filter(isNull(oktaId) || trim(oktaId) == '' || upper(trim(oktaId)) == 'NULL') ~> Validation",
				"Validation sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> GenertaeStageFile"
			]
		}
	}
}